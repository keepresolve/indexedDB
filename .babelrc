{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["Chrome >= 49", "ie >=8"]
            },
            "modules": "commonjs",
            "debug": true
          }
        ]
      ],
      "plugins": [
        [
          "transform-runtime",
          {
            "polyfill": true,
            "regenerator": true
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["Chrome >= 49", "last 2 Firefox versions"]
            },
            "modules": "commonjs",
            "debug": true
          }
        ]
      ],
      "plugins": [
        [
          "transform-runtime",
          {
            "polyfill": true,
            "regenerator": true
          }
        ]
      ]
    }
  }
}
